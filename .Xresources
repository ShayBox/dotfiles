! XFT

Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

! URXVT

urxvt*font: xft:Inconsolata:pixelsize=14,xft:Inconsolata for Powerline:pixelsize=14
urxvt*letterSpace: -1
urxvt.lineSpace: 0
urxvt.depth: 32
urxvt*scrollBar: false
!urxvt.geometry: 90x30
!urxvt.internalBorder: 3
!urxvt*transparent: true

urxvt.iso14755: false
urxvt.iso14755_52: false

urxvt.perl-ext-common: ibeamcursor,clipboard
urxvt.keysym.Shift-Control-V: perl:clipboard:paste
urxvt.keysym.Shift-Control-C: perl:clipboard:copy

! COLORS

! special
*.foreground:   #f8f8f8
*.background:   #171717
*.cursorColor:  #f8f8f8

! black
*.color0:       #171717
*.color8:       #38252c

! red
*.color1:       #d81765
*.color9:       #ff0000

! green
*.color2:       #97d01a
*.color10:      #76b639

! yellow
*.color3:       #ffa800
*.color11:      #e1a126

! blue
*.color4:       #16b1fb
*.color12:      #289cd5

! magenta
*.color5:       #ff2491
*.color13:      #ff2491

! cyan
*.color6:       #0fdcb6
